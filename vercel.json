{
  "version": 2,
  "builds": [
    {
      "src": "project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "backend/smartpay_backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/smartpay_backend/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "backend/smartpay_backend/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "backend/smartpay_backend/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "project/index.html"
    }
  ],
  "env": {
    "DEBUG": "False",
    "DJANGO_SETTINGS_MODULE": "smartpay_backend.settings.production"
  },
  "functions": {
    "backend/smartpay_backend/wsgi.py": {
      "runtime": "python3.9"
    }
  }
}
